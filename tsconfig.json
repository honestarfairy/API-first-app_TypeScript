{
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "lib": ["esnext", "dom"],
    "jsx": "react",
    "sourceMap": true,
    "target": "esnext",
    "removeComments": true,
    "outDir": "./dist",
    "moduleResolution": "node"
  }
}
